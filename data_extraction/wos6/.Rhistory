View(wos6)
wos6 <- read.csv("input/wos6.csv")
wos6$percent_survival <- as.numeric(wos6$percent_survival)
wos6$treatment <- as.factor(wos6$treatment)
wos6$concentration <- as.numeric(wos6$concentration)
levels(wos6$treatment)
ai_form_mod <- drm(percent_survival ~ concentration, treatment, data = wos6, fct = LL.2())
summary(ai_form_mod)
plot(ai_form_mod)
ai_form_mod <- drm(percent_survival ~ concentration, treatment, data = wos6, fct = LL.4())
summary(ai_form_mod)
plot(ai_form_mod)
ED(ai_form_mod, c(50), interval = "delta")
ai_form_mod <- drm(percent_survival ~ concentration, treatment, data = wos6, fct = LL.4(), type = "normal")
# model the ai and formulation together
ai_form_mod <- drm(percent_survival ~ concentration, treatment, data = wos6, fct = LL.4(), type = "continuous")
summary(ai_form_mod)
plot(ai_form_mod)
ED(ai_form_mod, c(50), interval = "delta")
