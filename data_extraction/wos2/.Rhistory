renv::init()
install.packages("drc")
knitr::opts_chunk$set(echo = TRUE)
wos2 <- read.csv("input/wos2.csv")
View(wos2)
wos2$alive <- as.numeric(wos2$alive)
wos2$total <- as.numeric(wos2$total)
wos2$dose <- as.numeric(wos2$dose)
wos2 <- drm(alive/total ~ dose, data = wos2, fct = LL.2(), weights = total, type = "binomial")
library(drc)
wos2_mod <- drm(alive/total ~ dose, data = wos2, fct = LL.2(), weights = total, type = "binomial")
summary(wos2_mod)
plot(wos2_mod)
wos2_mod <- drm(alive/total ~ dose, data = wos2, fct = LL.4(), weights = total, type = "binomial")
summary(wos2_mod)
plot(wos2_mod)
ED(wos2_mod, c(50), interval = "delta")
